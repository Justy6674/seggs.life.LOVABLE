{
  "name": "seggs-life-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for seggs.life couples analysis",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
