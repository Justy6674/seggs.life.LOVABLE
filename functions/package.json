  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
